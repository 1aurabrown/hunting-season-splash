@font-face {
  font-family: "Lineto Brown Regular Sub";
  src: url({{ "lineto-brown-regular.eot" | asset_url }});
  src: url({{ "lineto-brown-regular.eot?#iefix" | asset_url }}) format("embedded-opentype"),
       url({{ "lineto-brown-regular.woff2" | asset_url }}) format("woff2"),
       url({{ "lineto-brown-regular.woff" | asset_url }}) format("woff");
  font-weight: normal;
  font-style: normal;
}

